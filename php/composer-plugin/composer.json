{
  "name": "goopil/rabbit-rs-installer",
  "description": "Composer plugin that downloads prebuilt RabbitRs PHP extension binaries.",
  "type": "composer-plugin",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Goopil\\\\RabbitRs\\\\Installer\\\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.3"
  },
  "extra": {
    "class": "Goopil\\RabbitRs\\Installer\\Plugin"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.org"
    }
  ]
}
